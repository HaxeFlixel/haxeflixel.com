---
title: "Demos"
layout: "demos-layout"
---

<div class="demo-page center-align-a">

	<% for demo in @getCollection('demos').toJSON(): %>

	<a href="<%= demo.url %>" property="dc:title">
		<p><%= demo.title %></p>
		
		<img src="<%= @getThumbnail("/images/demos/#{demo.title}.png", "zoomcrop", { w: 190, h:190 }) %>" alt="<%= demo.title %>" width="190" height="190">
	</a>

	<% end %>

</div>

